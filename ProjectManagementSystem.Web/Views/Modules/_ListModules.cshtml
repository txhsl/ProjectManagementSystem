@model IEnumerable<ProjectManagementSystem.Modules.Dto.ModuleDto>
<div>
    <ul class="list-group">
        @foreach (var module in Model)
        {
            <li class="list-group-item">
                <div class="btn-group pull-right">
                    <button type="button" class="btn btn-info" onclick="editModule(@module.Id);">Edit</button>
                    <button type="button" class="btn btn-success" onclick="deleteModule(@module.Id);">Delete</button>
                </div>
                <div class="media">
                    <a class="media-left" href="#">
                        <i class="fa @module.GetTaskLable() fa-3x"></i>
                    </a>
                    <div class="media-body">
                        <h4 class="media-heading">@module.Name</h4>
                        <p class="text-info">Belong to: @module.ProjectName</p>
                        <p class="text-info">Principal：@module.MemberName</p>
                        <p class="text-muted">StartDate: @module.StartTime.ToString("yyyy-MM-dd HH:mm:ss")</p>
                        <p class="text-muted">DeliverDate: @module.DeliverTime.ToString("yyyy-MM-dd HH:mm:ss")</p>
                    </div>
                </div>
            </li>
        }
    </ul>
</div>